<h3>User List</h3>

<table class = "table table-striped">
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email</th>
			<th>Date Added</th>
		</tr>
	</thead>
	<tbody>
		{{#each users}}
		<tr>
			<td>{{firstname}}</td>
			<td>{{lastname}}</td>
			<td>{{email}}</td>
			<td>{{formatDate dateAdded}}</td>
		</tr>
		{{/each}}
	</tbody>
</table>




	

